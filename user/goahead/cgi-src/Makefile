#ALL_EXE = $(DOWNLOAD_URCP_SETTINGS) $(UPLOAD_BOOTLOADER_CGI) $(UPLOAD_SETTINGS) $(CLEAR_UPLOAD_SETTINGS) $(RRDTOOL_CGIWARPPER) $(CPU_MEM_RRD_UPDATER) $(NETWORK_RRD_UPDATER) 
