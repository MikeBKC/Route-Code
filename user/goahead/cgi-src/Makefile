#	$(STRIP) --remove-section=.note --remove-section=.comment $@

$(USB_UPGRADE_CGI): usb_upgrade.cgi.o crc32.o
	$(CC) $(CFLAGS) crc32.o usb_upgrade.cgi.o -o $@
	$(STRIP) --remove-section=.note --remove-section=.comment $@

$(DOWNLOAD_URCP_SETTINGS): exportLinkageConf.cgi.o
	$(CC) -L$(ROOTDIR)/lib/lib $(USB_FLAGE) $(CFLAGS) exportLinkageConf.cgi.o -o $@ 
	$(STRIP) --remove-section=.note --remove-section=.comment $@
ifneq ("$(CONFIG_UTT_AC)", "")
$(DOWNLOAD_AP_UPDATE_CONF): exportApUpdateConf.cgi.o crc32.o
	$(CC) -L$(ROOTDIR)/lib/lib $(USB_FLAGE) $(CFLAGS) exportApUpdateConf.cgi.o crc32.o -o $@ 
	$(STRIP) --remove-section=.note --remove-section=.comment $@
endif
