CONF	= $(ROOTDIR)/$(LINUXDIR)
ZLIB_H  = $(ROOTDIR)/lib/zlib
CONF_H  = $(ROOTDIR)/$(LINUXDIR)/include/linux/autoconf.h
CFLAGS += -I$(ROOTDIR)/uttShareHead/config

#
#  As you can see, RRDTool(traffic graphic) is obsoleted.
